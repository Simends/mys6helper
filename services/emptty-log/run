#!/usr/bin/execlineb -P
foreground { if -n -t { test -d /var/log/emptty } install -d -m 0755 -o s6log -g s6log /var/log/emptty }
foreground { chmod -R 755 /var/log/emptty }
envfile /etc/s6/sv/emptty-log/conf
importas -sCiu DIRECTIVES DIRECTIVES
s6-setuidgid s6log exec -c s6-log -d3 -b -- ${DIRECTIVES} /var/log/emptty
